<script>

function uncheck(elem) {
    var op_id = elem.id;
    var theClass = elem.className;

    if (theClass.includes("pollOptionsUncheck") == true) {
        $(".pollOptions").attr("checked", false);
        $( "input:checkbox:not(:checked)").attr( "checked", false);
    } else {
        $(".pollOptionsUncheck").attr("checked", false);
        $("input:checkbox:not(:checked)").attr("checked", false);
    }
}

	function resetPollForm()
	{
		$("#radioForm input").prop("checked", false);
	}

