$("#radioForm input").click(function() {

	var input_id = $(this).attr("id");
	var checkStatus = $("#"+input_id).prop("checked");

	if (checkStatus == "checked") {

		if (input_id >12587 && input_id < 12644) {
			$("#12587").prop("checked", false);
			$("#12644").prop("checked", false);
		}

        if (input_id >12645 && input_id < 12657) {
			$("#12587").prop("checked", false);
			$("#12644").prop("checked", false);
		}

		if (input_id == 12587 || input_id == 12644) {
			var start = 12590;
			var end = 12643;

			for(start; start < end;) {
				$("#"+start).prop("checked", false);
				start = start +3;
			}

            $("#12647").prop("checked", false);
            $("#12650").prop("checked", false);
            $("#12653").prop("checked", false);
            $("#12656").prop("checked", false);

			if (input_id==12587) {
				$("#12644").prop("checked", false);
			}

			if(input_id == 12644) {
				$("#12587").prop("checked", false);
			}
		}
	}

});


function SubmitCheckForm2(radioForm) {

    var frmCheckform = document.radioForm;
	var ticks = document.getElementsByName("options[]");
	var selectedOptions = false;
	var max_choice = 1;
	var total_tick = 0;

	for (var i = 0; i < ticks.length; i++) {
		if (ticks[i].checked) {
            selectedOptions = true;
            total_tick = total_tick + 1;
		}
	}

    if ((total_tick == 0)) {
        alert("You must select an answer in order to vote.");
        return false;
    } else if (total_tick > max_choice) {
        alert("It looks like you selected too many options. The maximum you are allowed is " + max_choice + ". You selected " + total_tick);
        return false;
    }if ((!selectedOptions) || total_tick < max_choice) {
    alert("Please select one option for every service listed.");
    return false;
}

if (total_tick > max_choice ) {
    alert("It looks like you selected too many options. The maximum you are allowed is " + max_choice + ". You selected " + total_tick);
    return false;
} else if ((!selectedOptions)) {
    alert("You must select an answer in order to vote.");
    return false;
} else if (total_tick == max_choice && selectedOptions) {
    $(".chosen2201").attr("disabled", false);
    $(".chosen2204").attr("disabled", false);
    proceedWithForm == true;
}

if(proceedWithForm == true) {
    document.radioForm.submit();
} radioForm.submit();
}
